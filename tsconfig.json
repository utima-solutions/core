{
  "compilerOptions": {
    "target": "ES2018",
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "typeRoots": [
      "./node_modules/@types"
    ],
  }
}
